{
  "name": "@kinsi/anyrpc",
  "version": "1.0.4",
  "main": "dist/esm/anyrpc.js",
  "exports": {
    "require": "./dist/cjs/anyrpc.js",
    "import": "./dist/esm/anyrpc.js"
  },
	"files": [
		"dist"
	],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
